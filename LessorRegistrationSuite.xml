    <test name="LessorRegTestCase10">
        <parameter name="test_param" value="Lessor_REG-0010"/>
        <classes>
            <class name="Test.LessorRegistration.InvalidInputLessorContactTab"/>
        </classes>
    </test>


    <test name="LessorRegTestCase11">
        <parameter name="test_param" value="Lessor_REG-011"/>
        <classes>
            <class name="Test.LessorRegistration.BlankInputLessorComplianceTab"/>
        </classes>
    </test>


    <test name="LessorRegTestCase12">
        <parameter name="test_param" value="Lessor_REG-012"/>
        <classes>
            <class name="Test.LessorRegistration.InvalidInputLessorComplianceTab"/>
        </classes>
