<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Lessor Registration Test Case Suite" parallel="methods" thread-count="3">
    <listeners>
        <listener class-name="TestComponents.Listners"/>
    </listeners>
    <parameter name="excel_Sheet" value="Lessor_Registration"/>

    <!--Verifying the Positive Flow of Lessor Registration-->
    <test name="LessorRegTestCase1">
        <parameter name="test_param" value="Lessor_REG-001"/>
        <classes>
            <class name="Test.LessorRegistration.LessorRegistrationFlow"/>

        </classes>
    </test>

    <!--    Verifying the Basic Tab of Lessor Registation Flow on Blank Input -->
    <test name="LessorRegTestCase2">
        <parameter name="test_param" value="Lessor_REG-002"/>
        <classes>
            <class name="Test.LessorRegistration.BlankInputLessorBasicTab"/>
        </classes>
    </test>

    <!--Verifying the Basic Tab on Invalid Input-->
    <test name="LessorRegTestCase3">
        <parameter name="test_param" value="Lessor_REG-003"/>
        <classes>
            <class name="Test.LessorRegistration.InvalidInputLessorBasicTab"/>
        </classes>
    </test>


    <!--Verifying the Address Tab on Blank Input-->
    <test name="LessorRegTestCase4">
        <parameter name="test_param" value="Lessor_REG-004"/>
        <classes>
            <class name="Test.LessorRegistration.BlankInputLessorAddressTab"/>
        </classes>
    </test>


    <!--Verifying Editing of the Address Tab Details-->
    <test name="LessorRegTestCase5">
        <parameter name="test_param" value="Lessor_REG-005"/>
        <classes>
            <class name="Test.LessorRegistration.EditLessorAddrsDetails"/>
        </classes>
    </test>


    <!--Verifying Add of Multiple Address with Different Address Type-->
    <test name="LessorRegTestCase6">
        <parameter name="test_param" value="Lessor_REG-006"/>
        <classes>
            <class name="Test.LessorRegistration.RepeatedDetailsDiffAddrsType"/>
        </classes>
    </test>

    <!--Verifying Add of Similar Address Details Twice-->
    <test name="LessorRegTestCase7">
        <parameter name="test_param" value="Lessor_REG-007"/>
        <classes>
            <class name="Test.LessorRegistration.RepeatedDetailsAddrsTab"/>
        </classes>
    </test>

    <!--Verify Saving of Contact Details on Blank Fields and Verifying Error Messages are disappearing after correct text input-->
    <test name="LessorRegTestCase8">
        <parameter name="test_param" value="Lessor_REG-008"/>

        <classes>
            <class name="Test.LessorRegistration.BlankInputLessorContactTab"/>
        </classes>
    </test>

